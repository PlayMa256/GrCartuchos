{
  "parser": "babel-eslint",
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/prop-types": "off",
    "react/jsx-no-target-blank": "off",
    "react/display-name": "off",
    "react/jsx-no-duplicate-props": "off"
  }
}